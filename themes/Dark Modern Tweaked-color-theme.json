{
	"name": "Dark Modern Tweaked",
  "include": "./dark_modern.json",
	"colors": {
    "editorBracketHighlight.foreground1": "#f17cbf",
    "editorBracketHighlight.foreground2": "#f7aad5",
    "editorBracketHighlight.foreground3": "#aca9c7",
    "editorBracketHighlight.foreground4": "#b58adf",
	},
	"tokenColors": [
    {
      "name": "Type (Java), Primitive Type (Java)",
      "scope": [
        "storage.type.primitive.java"
      ],
      "settings": {
        "foreground": "#f7aad5"
      }
    },
    {
      "name": "Class Type (Java)",
      "scope": [
        "entity.name.type.class",
      ],
      "settings": {
        "foreground": "#e9e9e9"
      }
    },
    {
			"name": "Storage Modifier (Java), Import Keyword (Java), Package Keyword (Java)",
			"scope": [
				"storage.modifier.java",
        "keyword.other.import.java",
        "keyword.other.package.java"
			],
			"settings": {
        "foreground": "#f17cbf"
			}
		},
		{
			"name": "Import (Java), Package (Java), Source (Java)",
			"scope": [
        "entity.name.namespace",
				"storage.modifier.package.java",
        "meta.package.java",
        "storage.modifier.import.java",
        "meta.import.java",
        "source.java"
			],
			"settings": {
        "foreground": "#e9e9e9"
			}
		},
		{
			"name": "Constant Variable Property",
			"scope": [
        "variable.other.constant.property",
			],
			"settings": {
        "fontStyle": "italic",
        "foreground": "#b58adf"
			}
		},
		{
			"name": "Keyword Control (Java)",
			"scope": [
        "keyword.control.java",
			],
			"settings": {
        "foreground": "#e094e7"
			}
		},
		{
			"name": "Function Member (Java)",
			"scope": [
        "entity.name.function.member",
			],
			"settings": {
        "foreground": "#8bf3a5"
			}
		},
		{
			"name": "String (Java), Char (Java)",
			"scope": [
        "string.quoted.double.java",
        "string.quoted.single.java"
			],
			"settings": {
        "foreground": "#e0e6a8"
			}
		},
    {
      "name": "Numbers (Java)",
      "scope": [
        "constant.numeric.decimal.java",
      ],
      "settings": {
        "foreground": "#bda5df",
      }
    },
    {
      "name": "Variables (Java)",
      "scope": [
        "variable.other.definition.java",
        "meta.definition.variable.java",
      ],
      "settings": {
        "foreground": "#bda5df",
      }
    },
    {
      "name": "Punctuation (Java)",
      "scope": [
        "punctuation.separator.period.java",
        "punctuation.terminator.java",
      ],
      "settings": {
        "foreground": "#A9B7C6",
      }
    },

	],
  "semanticTokenColors": {
    "*.importDeclaration": "#e9e9e9",
    "variable:java": "#a8eaec",
    "parameter:java": "#a8eaec"
  }
}
